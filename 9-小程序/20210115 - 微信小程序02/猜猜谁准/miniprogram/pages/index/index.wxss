/**index.wxss**/
.avatar {
  border: none;
  background: none;
  padding: 0;
  line-height: 0;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  text-align: center;
}
.avatar:after {
  border: none;
}
.player-image {
  border-radius: 50%;
  width: 200rpx;
  height: 200rpx;
}
#nickName{
  margin-bottom: 60rpx;
  text-align: center;
}
.btnWrapper{
  margin-bottom: 60rpx;
  text-align: center;
}



#room{
  display: flex;
  flex-wrap: wrap;
  border: 1px solid #ddd;
  min-height: 264rpx;
  margin-bottom: 60rpx;
}
.player{
  box-sizing: border-box;
  width: 88rpx;
  height: 88rpx;
  background-color: #ddd;
  border: 2px solid #fff;
  border-radius: 9rpx;
}






